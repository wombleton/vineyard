{
  "plugins": [
    "syntax-dynamic-import",
    "transform-decorators-legacy",
    "transform-class-properties"
  ],
  "presets": [
    [
      "env",
      {
        "targets": { "browsers": ["last 2 versions", "safari >= 7"] },
        "modules": false,
        "loose": true
      }
    ],
    "react"
  ],
  "env": {
    "development": {
      "plugins": ["react-hot-loader/babel"]
    }
  }
}
